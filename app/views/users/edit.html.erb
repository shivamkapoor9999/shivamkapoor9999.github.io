<h1>Edit Your Profile</h1>
<%= form_for @user,:url => edituser_path do |u|%>
	<%= u.label :email %>
	<%= u.text_field :email%>
	<br>
	<%= u.label :name %>
	<%= u.text_field :name%>
	<br>
	<%= u.label :age %>
	<%= u.number_field :age%>
	<br>
	<%= u.label :gender %>
	<%= u.text_field :gender%>
	<br>
	<%= u.label :password %>
	<%= u.password_field :password %>
	<br>
	<%= u.submit %>
<%end%>
